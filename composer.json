{
    "name": "uvdesk/community-skeleton",
    "description": "UVDesk Community Helpdesk Project Skeleton",
    "type": "project",
    "license": "MIT",
    "require": {
        "php": "^7.2 || ^8.0",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "composer/package-versions-deprecated": "1.11.99.3",
        "doctrine/annotations": "^1.0",
        "doctrine/doctrine-bundle": "^2.4",
        "doctrine/doctrine-fixtures-bundle": "^3.4.0",
        "doctrine/doctrine-migrations-bundle": "^2.2.2",
        "doctrine/orm": "^2.9",
        "google/recaptcha": "^1.2.4",
        "knplabs/knp-paginator-bundle": "^5.4.0",
        "ocramius/proxy-manager": "2.2.3",
        "phpdocumentor/reflection-docblock": "^5.2",
        "sensio/framework-extra-bundle": "^5.5.3",
        "symfony/asset": "4.4.*",
        "symfony/console": "4.4.*",
        "symfony/dotenv": "4.4.*",
        "symfony/expression-language": "4.4.*",
        "symfony/flex": "^1.12.2",
        "symfony/form": "^4.4.25",
        "symfony/framework-bundle": "4.4.*",
        "symfony/http-client": "4.4.*",
        "symfony/intl": "4.4.*",
        "symfony/mime": "4.4.*",
        "symfony/monolog-bundle": "^3.6.0",
        "symfony/process": "4.4.*",
        "symfony/property-access": "^4.4",
        "symfony/property-info": "^4.4",
        "symfony/proxy-manager-bridge": "^4.4",
        "symfony/security-bundle": "4.4.*",
        "symfony/serializer": "^4.4",
        "symfony/swiftmailer-bundle": "^3.4.0",
        "symfony/translation": "4.4.*",
        "symfony/twig-bundle": "4.4.*",
        "symfony/validator": "4.4.*",
        "symfony/var-dumper": "4.4.*",
        "symfony/web-link": "4.4.*",
        "symfony/yaml": "4.4.*",
        "uvdesk/automation-bundle": "@dev",
        "uvdesk/composer-plugin": "@dev",
        "uvdesk/core-framework": "@dev",
        "uvdesk/extension-framework": "dev-master",
        "uvdesk/mailbox-component": "@dev",
        "uvdesk/support-center-bundle": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "symfony/browser-kit": "^4.4",
        "symfony/css-selector": "^4.4",
        "symfony/debug-bundle": "^4.4",
        "symfony/maker-bundle": "^1.0",
        "symfony/phpunit-bridge": "^5.3",
        "symfony/stopwatch": "^4.4",
        "symfony/web-profiler-bundle": "4.4.*",
        "symfony/web-server-bundle": "4.4.*",
        "uvdesk/api-bundle": "@dev"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        }
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        },
        "symfony": {
            "allow-contrib": false,
            "require": "4.4.*"
        }
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
